{
  "redhat.telemetry.enabled": false,
  "ansible.python.activationScript": "${HOME}/python-venvs/ansible/bin/activate",
  "ansible.python.interpreterPath": "${HOME}/python-venvs/ansible/bin/python",
  "ansible.ansible.path": "/${HOME}/python-venvs/ansible/bin/ansible",
  "python.terminal.activateEnvironment": true,
  "files.associations": {
    "**/defaults/*.y*ml": "ansible",
    "**/group_vars/**/*.y*ml": "ansible",
    "**/handlers/*.y*ml": "ansible",
    "**/host_vars/**/*.y*ml": "ansible",
    "**/inventory/**": "toml",
    "**/inventory/**/*.y*ml": "ansible",
    "**/meta/*.y*ml": "ansible",
    "**/molecule/**/converge.y*ml": "ansible",
    "**/molecule/**/molecule.y*ml": "yaml",
    "**/molecule/**/verify.y*ml": "ansible",
    "**/tasks/*.y*ml": "ansible",
    "**/templates/*.j2": "ansible-jinja",
    "**/tests/*.y*ml": "ansible",
    "**/tests/inventory": "toml",
    "**/vars/*.y*ml": "ansible",
    "**/playbooks/*.y*ml": "ansible",
    ".ansible-lint": "yaml",
    ".yamllint": "yaml",
    ".pre-commit-config.y*ml": "yaml",
    "ansible.cfg": "properties",
    "play*.y*ml": "ansible",
  },
}
